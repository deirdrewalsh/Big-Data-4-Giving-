{
  "name": "big-data-4-giving",
  "version": "1.0.0",
  "description": "Map application for overlaying data to help corporate donors make data-driven donation decisions in Austin, TX",
  "main": "index.js",
  "dependencies": {
    "backbone": "^1.3.3",
    "bootstrap": "^3.3.6",
    "jquery": "^2.2.4",
    "leaflet": "^0.7.7",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "webpack": "^1.13.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "img": "mkdir -p dist/imgs && cp -r src/imgs/ dist/imgs/",
    "css": "mkdir -p dist/styles && cp -r src/styles/ dist/styles/",
    "js": "webpack src/scripts/main.js dist/scripts/main.js",
    "html": "cp src/index.html dist/index.html",
    "build": "npm run js && npm run html && npm run css && npm run img",
    "prestart": "npm run build",
    "start": "http-server dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deirdrewalsh/Big-Data-4-Giving-.git"
  },
  "keywords": [
    "atxhack4change"
  ],
